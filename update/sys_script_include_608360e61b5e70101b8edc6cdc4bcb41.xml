        <description>on demand Script Include to validate eemail address syntax with regex</description>
        <name>validateEmailAddress</name>
        <script><![CDATA[function validateEmailAddress(emailStr) {
	emailStr = emailStr + ''; // coerce String type
	return emailStr.match(/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/);
}]]></script>
